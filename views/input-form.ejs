<%-include('header')%>
    <a style="float: left;" class="btn btn-secondary" href="/io-info">
        ← 목록으로
    </a><br><br><br>

    <h3>키보드 정보 입력</h3>
    <hr>
    <form action="create" method="POST">
        <div class="mb-3 row">
            <label for="name" class="col-sm-2 col-form-label" style="text-align: right;">제품명</label>
            <div class="col-sm-8">
                <input type="text" class="form-control" id="name" name='name' placeholder="제품명">
            </div>
        </div>
        <div class="mb-3 row">
            <label for="brand" class="col-sm-2 col-form-label" style="text-align: right;">브랜드명</label>
            <div class="col-sm-8">
                <input type="text" class="form-control" id="brand" name='brand' placeholder="브랜드명">
            </div>
        </div>
        <div class="mb-3 row">
            <label for="keytype" class="col-sm-2 col-form-label" style="text-align: right;">키보드 타입</label>
            <div class="col-sm-8">
                <input type="text" class="form-control" id="keytype" name='keytype' placeholder="키보드 타입">
            </div>
        </div>
        <div class="mb-3 row">
            <label for="status" class="col-sm-2 col-form-label" style="text-align: right;">상태</label>
            <div class="col-sm-8">
                <input type="text" class="form-control" id="state" name='status' placeholder="상태">
            </div>
        </div>
        <div class="mb-3 row">
            <label for="code" class="col-sm-2 col-form-label" style="text-align: right;">코드</label>
            <div class="col-sm-8">
                <input type="text" class="form-control" id="code" name='code' placeholder="코드">
            </div>
        </div>
        <div class="mb-3 row">
            <label for="price" class="col-sm-2 col-form-label" style="text-align: right;">가격</label>
            <div class="col-sm-8">
                <input type="text" class="form-control" id="price" name='price' placeholder="가격">
            </div>
        </div>

        <!-- <div class="mb-3 row">
            <label for="cate_id" class="col-sm-2 col-form-label" style="text-align: right;">cate</label>
            <div class="col-sm-8">
                <select class="form-select" aria-label="cate_id" id="cate_id" name='cate_id'>
